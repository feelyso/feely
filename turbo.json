{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "NEXT_PUBLIC_BASE_URL"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "env": [
        "NEXT_PUBLIC_BASE_URL"
      ],
      "outputs": [
        "dist/**",
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "@feely/storybook#build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "storybook-static/**",
        "!storybook-static/cache/**"
      ]
    },
    "preview": {
      "env": [
        "NEXT_PUBLIC_BASE_URL"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ],
      "cache": false
    },
    "start": {
      "dependsOn": [
        "^build"
      ]
    },
    "lint": {
      "cache": false,
      "outputs": []
    },
    "lint:fix": {
      "cache": false,
      "outputs": []
    },
    "typecheck": {},
    "dev": {
      "env": [
        "NEXT_PUBLIC_BASE_URL"
      ],
      "cache": false
    },
    "check": {
      "cache": false
    },
    "test": {
      "cache": false,
      "outputs": []
    }
  }
}
